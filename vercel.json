{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "env": [
    {
      "key": "VITE_SUPABASE_URL",
      "value": "@supabase_url"
    },
    {
      "key": "VITE_SUPABASE_ANON_KEY",
      "value": "@supabase_anon_key"
    },
    {
      "key": "VITE_API_BASE_URL",
      "value": "@api_base_url"
    },
    {
      "key": "VITE_ENV",
      "value": "production"
    }
  ]
}
